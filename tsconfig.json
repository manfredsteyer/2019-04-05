      ],
      "@asti/payment/data-payment": ["libs/payment/data-payment/src/index.ts"],
      "@asti/message/data-message": ["libs/message/data-message/src/index.ts"],
      "@asti/message/shared-message": [
        "libs/message/shared-message/src/index.ts"
      ],
      "@asti/payment/shared-payment": [
        "libs/payment/shared-payment/src/index.ts"
      ],
      "@asti/shared/ui-common": ["libs/shared/ui-common/src/index.ts"],
      "@asti/shared/ui-technical-protocol": [
        "libs/shared/ui-technical-protocol/src/index.ts"
      ],
      "@asti/shared/ui-outbound-messages": [
        "libs/shared/ui-outbound-messages/src/index.ts"
      ],
      "@asti/message/api-message": ["libs/message/api-message/src/index.ts"],
